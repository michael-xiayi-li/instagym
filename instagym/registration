		function submitfindgym(){

			var userfirstname = document.getElementById("firstname").value;
 			var userlastname = document.getElementById("lastname").value;
 			var usercontact = document.getElementById("e-mail").value;
 			var userprice= document.getElementById("price").value;
 			var useraddressline1 = document.getElementById("addressline1").value;
 			var useraddressline2 = document.getElementById("addressline2").value;
 			var usercity = document.getElementById("city").value;
 			var userstate = document.getElementById("state").value;
 			var userzipcode = document.getElementById("zipcode").value;
 			var userdescription = document.getElementById("descriptions").value;

 			submitgym(userfirstname,userlastname,usercontact,userprice,userzipcode,userdescription,useraddressline1,useraddressline2,usercity,userstate);

 			location.href = "thanksforregistrating.html";
 			console.log("wtf");
